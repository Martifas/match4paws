m,\n  } = usePetForm();\n\n  useEffect(() => {\n    if (pet && open) {\n      // 1️⃣ clear any previous data\n      resetForm?.(); // <- no args\n\n      // 2️⃣ patch in the existing values\n      updateField('name', pet.name ?? '');\n      updateField('type', pet.type ?? 'dog');\n      updateField('breed', pet.breed ?? '');\n      updateField('gender', pet.gender ?? 'female');\n      updateField('size', pet.size ?? 'medium');\n      updateField('ageGroup', pet.ageGroup ?? 'adult');\n      updateField('description', pet.description ?? '');\n      updateField('imageUrls', pet.imageUrls ?? []);\n    }\n  }, [pet, open, resetForm, updateField]);\n\n  const handleSubmit = async (e: React.FormEvent) => {\n    e.preventDefault();\n    await submitForm(data => onSubmit(data, pet?.id), onClose);\n  };\n\n  const isEdit = !!pet;\n\n  return (\n    <Dialog open={open} onClose={onClose} maxWidth=\"md\" fullWidth>\n      <DialogTitle>\n        <Box display=\"flex\" justifyContent=\"space-between\" alignItems=\"center\">\n          {isEdit ? 'Edit Pet' : 'Add New Pet'}\n          <IconButton onClick={onClose}>\n            <Close />\n          </IconButton>\n        </Box>\n      </DialogTitle>\n\n      <form onSubmit={handleSubmit}>\n        <DialogContent>\n          <Grid container spacing={3}>\n            <Grid size={{ xs: 12, sm: 6 }}>\n              <TextField\n                required\n                fullWidth\n                label=\"Pet Name\"\n                value={formData.name}\n                onChange={e => updateField('name', e.target.value)}\n              />\n            </Grid>\n\n            <Grid size={{ xs: 12, sm: 6 }}>\n              <FormSelect\n                label=\"Type\"\n                value={formData.type}\n                onChange={value => updateField('type', value)}\n                options={PET_TYPES}\n                required\n              />\n            </Grid>\n\n            <Grid size={{ xs: 12, sm: 6 }}>\n              <TextField\n                fullWidth\n                label=\"Breed\"\n                value={formData.breed}\n                onChange={e => updateField('breed', e.target.value)}\n                placeholder=\"e.g., Golden Retriever, Persian\"\n              />\n            </Grid>\n\n            <Grid size={{ xs: 12, sm: 6 }}>\n              <FormSelect\n                label=\"Gender\"\n                value={formData.gender}\n                onChange={value => updateField('gender', value)}\n                options={GENDERS}\n                required\n              />\n            </Grid>\n\n            <Grid size={{ xs: 12, sm: 6 }}>\n              <FormSelect\n                label=\"Size\"\n                value={formData.size}\n                onChange={value => updateField('size', value)}\n                options={SIZES}\n                required\n              />\n            </Grid>\n\n            <Grid size={{ xs: 12, sm: 6 }}>\n              <FormSelect\n                label=\"Age Group\"\n                value={formData.ageGroup}\n                onChange={value => updateField('ageGroup', value)}\n                options={AGES}\n                required\n              />\n            </Grid>\n\n            <Grid size={{ xs: 12 }}>\n              <TextField\n                fullWidth\n                multiline\n                rows={4}\n                label=\"Description\"\n                value={formData.description}\n                onChange={e => updateField('description', e.target.value)}\n                placeholder=\"Tell potential adopters about this pet's personality, habits, medical needs, etc.\"\n              />\n            </Grid>\n\n            <Grid size={{ xs: 12 }}>\n              <ImageUrlManager\n                imageUrls={formData.imageUrls}\n                onUrlsChange={urls => updateField('imageUrls', urls)}\n              />\n            </Grid>\n          </Grid>\n        </DialogContent>\n\n        <DialogActions>\n          <Button onClick={onClose}>Cancel</Button>\n          <PrimaryButton\n            type=\"submit\"\n            disabled={!isFormValid || isSubmitting}\n            fullWidth={false}\n            className=\"px-6\"\n          >\n            {isSubmitting\n              ? isEdit\n                ? 'Saving…'\n                : 'Adding…'\n              : isEdit\n                ? 'Save Changes'\n                : 'Add Pet'}\n          </PrimaryButton>\n        </DialogActions>\n      </form>\n    </Dialog>\n  );\n}\n"],"mappings":";AA4EY;AA1EZ;AAAA,EACE;AAAA,EACA;AAAA,EACA;AAAA,EACA;AAAA,EACA;AAAA,EACA;AAAA,EACA;AAAA,EACA;AAAA,EACA;AAAA,OACK;AACP,SAAS,aAAa;AAEtB,SAAS,MAAM,SAAS,WAAW,aAAa;AAChD,SAAS,kBAAkB;AAC3B,SAAS,uBAAuB;AAChC,SAAS,kBAA+B;AACxC,OAAO,mBAAmB;AAC1B,SAAS,iBAAiB;AAW1B,wBAAwB,YAAY;AAAA,EAClC;AAAA,EACA;AAAA,EACA;AAAA,EACA,MAAM;AACR,GAAqB;AACnB,QAAM;AAAA,IACJ;AAAA,IACA;AAAA,IACA;AAAA,IACA;AAAA,IACA;AAAA,IACA;AAAA,EACF,IAAI,WAAW;AAEf,YAAU,MAAM;AACd,QAAI,OAAO,MAAM;AAEf,kBAAY;AAGZ,kBAAY,QAAQ,IAAI,QAAQ,EAAE;AAClC,kBAAY,QAAQ,IAAI,QAAQ,KAAK;AACrC,kBAAY,SAAS,IAAI,SAAS,EAAE;AACpC,kBAAY,UAAU,IAAI,UAAU,QAAQ;AAC5C,kBAAY,QAAQ,IAAI,QAAQ,QAAQ;AACxC,kBAAY,YAAY,IAAI,YAAY,OAAO;AAC/C,kBAAY,eAAe,IAAI,eAAe,EAAE;AAChD,kBAAY,aAAa,IAAI,aAAa,CAAC,CAAC;AAAA,IAC9C;AAAA,EACF,GAAG,CAAC,KAAK,MAAM,WAAW,WAAW,CAAC;AAEtC,QAAM,eAAe,OAAO,MAAuB;AACjD,MAAE,eAAe;AACjB,UAAM,WAAW,UAAQ,SAAS,MAAM,KAAK,EAAE,GAAG,OAAO;AAAA,EAC3D;AAEA,QAAM,SAAS,CAAC,CAAC;AAEjB,SACE,uBAAC,UAAO,MAAY,SAAkB,UAAS,MAAK,WAAS,MAC3D;AAAA,2BAAC,eACC,iCAAC,OAAI,SAAQ,QAAO,gBAAe,iBAAgB,YAAW,UAC3D;AAAA,eAAS,aAAa;AAAA,MACvB,uBAAC,cAAW,SAAS,SACnB,iCAAC,WAAD;AAAA;AAAA;AAAA;AAAA,aAAO,KADT;AAAA;AAAA;AAAA;AAAA,aAEA;AAAA,SAJF;AAAA;AAAA;AAAA;AAAA,WAKA,KANF;AAAA;AAAA;AAAA;AAAA,WAOA;AAAA,IAEA,uBAAC,UAAK,UAAU,cACd;AAAA,6BAAC,iBACC,iCAAC,QAAK,WAAS,MAAC,SAAS,GACvB;AAAA,+BAAC,QAAK,MAAM,EAAE,IAAI,IAAI,IAAI,EAAE,GAC1B;AAAA,UAAC;AAAA;AAAA,YACC,UAAQ;AAAA,YACR,WAAS;AAAA,YACT,OAAM;AAAA,YACN,OAAO,SAAS;AAAA,YAChB,UAAU,OAAK,YAAY,QAAQ,EAAE,OAAO,KAAK;AAAA;AAAA,UALnD;AAAA;AA