6":120,"7":3},"b":{"0":[451],"1":[7,2],"2":[9,9],"3":[1,417],"4":[452,108]},"inputSourceMap":{"version":3,"sources":["/home/marty/turing/4-module/4-sprint/match4paws/src/components/ui/forms/ImageUrlManager.tsx"],"sourcesContent":["import { useState } from \"react\";\nimport { Box, Typography, TextField, Button, IconButton } from \"@mui/material\";\nimport { Close } from \"@mui/icons-material\";\n\ntype ImageUrlManagerProps = {\n  imageUrls: string[];\n  onUrlsChange: (urls: string[]) => void;\n  label?: string;\n};\n\nexport function ImageUrlManager({\n  imageUrls,\n  onUrlsChange,\n  label = \"Photo URLs\",\n}: ImageUrlManagerProps) {\n  const [currentUrl, setCurrentUrl] = useState(\"\");\n\n  const addUrl = () => {\n    if (currentUrl.trim() && !imageUrls.includes(currentUrl.trim())) {\n      onUrlsChange([...imageUrls, currentUrl.trim()]);\n      setCurrentUrl(\"\");\n    }\n  };\n\n  const removeUrl = (urlToRemove: string) => {\n    onUrlsChange(imageUrls.filter((url) => url !== urlToRemove));\n  };\n\n  const handleKeyPress = (e: React.KeyboardEvent) => {\n    if (e.key === \"Enter\") {\n      e.preventDefault();\n      addUrl();\n    }\n  };\n\n  return (\n    <Box>\n      <Typography variant=\"subtitle2\" gutterBottom>\n        {label}\n      </Typography>\n\n      <Box display=\"flex\" gap={1} mb={2}>\n        <TextField\n          fullWidth\n          placeholder=\"Enter image URL\"\n          value={currentUrl}\n          onChange={(e) => setCurrentUrl(e.target.value)}\n          onKeyPress={handleKeyPress}\n        />\n        <Button\n          variant=\"outlined\"\n          onClick={addUrl}\n          disabled={!currentUrl.trim()}\n          sx={{ minWidth: \"auto\", px: 2 }}\n        >\n          Add\n        </Button>\n      </Box>\n\n      {imageUrls.length > 0 && (\n        <Box>\n          <Typography variant=\"body2\" color=\"text.secondary\" gutterBottom>\n            Added photos ({imageUrls.length}):\n          </Typography>\n          <Box display=\"flex\" flexDirection=\"column\" gap={1}>\n            {imageUrls.map((url, index) => (\n              <Box\n                key={index}\n                display=\"flex\"\n                alignItems=\"center\"\n                justifyContent=\"space-between\"\n                p={1}\n                border=\"1px solid #e0e0e0\"\n                borderRadius={1}\n              >\n                <Typography\n                  variant=\"body2\"\n                  sx={{\n                    overflow: \"hidden\",\n                    textOverflow: \"ellipsis\",\n                    whiteSpace: \"nowrap\",\n                    maxWidth: \"300px\",\n                  }}\n                >\n                  {url}\n                </Typography>\n                <IconButton\n                  size=\"small\"\n                  onClick={() => removeUrl(url)}\n                  color=\"error\"\n                >\n                  <Close fontSize=\"small\" />\n                </IconButton>\n              </Box>\n            ))}\n          </Box>\n        </Box>\n      )}\n    </Box>\n  );\n}\n"],"mappings":"AAqCM;AArCN,SAAS,gBAAgB;AACzB,SAAS,KAAK,YAAY,WAAW,QAAQ,kBAAkB;AAC/D,SAAS,aAAa;AAQf,gBAAS,gBAAgB;AAAA,EAC9B;AAAA,EACA;AAAA,EACA,QAAQ;AACV,GAAyB;AACvB,QAAM,CAAC,YAAY,aAAa,IAAI,SAAS,EAAE;AAE/C,QAAM,SAAS,MAAM;AACnB,QAAI,WAAW,KAAK,KAAK,CAAC,UAAU,SAAS,WAAW,KAAK,CAAC,GAAG;AAC/D,mBAAa,CAAC,GAAG,WAAW,WAAW,KAAK,CAAC,CAAC;AAC9C,oBAAc,EAAE;AAAA,IAClB;AAAA,EACF;AAEA,QAAM,YAAY,CAAC,gBAAwB;AACzC,iBAAa,UAAU,OAAO,CAAC,QAAQ,QAAQ,WAAW,CAAC;AAAA,EAC7D;AAEA,QAAM,iBAAiB,CAAC,MAA2B;AACjD,QAAI,EAAE,QAAQ,SAAS;AACrB,QAAE,eAAe;AACjB,aAAO;AAAA,IACT;AAAA,EACF;AAEA,SACE,uBAAC,OACC;AAAA,2BAAC,cAAW,SAAQ,aAAY,cAAY,MACzC,mBADH;AAAA;AAAA;AAAA;AAAA,WAEA;AAAA,IAEA,uBAAC,OAAI,SAAQ,QAAO,KAAK,GAAG,IAAI,GAC9B;AAAA;AAAA,QAAC;AAAA;AAAA,UACC,WAAS;AAAA,UACT,aAAY;AAAA,UACZ,OAAO;AAAA,UACP,UAAU,CAAC,MAAM,cAAc,EAAE,OAAO,KAAK;AAAA,UAC7C,YAAY;AAAA;AAAA,QALd;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MAMA;AAAA,MACA;AAAA,Q